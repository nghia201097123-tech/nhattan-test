{
  "name": "customer-gift",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "generate": "protoc -I=./src/v1/grpc/protos --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./src/v1/grpc/interfaces --ts_proto_opt=addGrpcMetadata=true --ts_proto_opt=nestJs=true --ts_proto_opt=snakeToCamel=false ./src/v1/grpc/protos/*.proto",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node --max_old_space_size=20480 dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "sonarqube": "sonar-scanner"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^9.0.1",
    "@grpc/grpc-js": "^1.10.6",
    "@grpc/proto-loader": "^0.7.12",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/bull": "^10.1.1",
    "@nestjs/common": "^10.3.8",
    "@nestjs/core": "^10.3.8",
    "@nestjs/elasticsearch": "^11.1.0",
    "@nestjs/microservices": "^10.3.8",
    "@nestjs/platform-express": "^10.3.8",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/typeorm": "^10.0.2",
    "jsonwebtoken": "^9.0.2",
    "kafka": "^0.2.3",
    "kafkajs": "^2.2.4",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "mongoose": "^8.9.1",
    "mysql2": "^3.10.0",
    "redis": "^4.7.0",
    "redlock": "^5.0.0-beta.2",
    "rxjs": "^7.8.1",
    "ts-proto": "^1.176.0",
    "typeorm": "^0.3.20",
    "undici": "^5.28.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/config": "^3.2.2",
    "@nestjs/mongoose": "^10.1.0",
    "@nestjs/platform-fastify": "^10.3.8",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/testing": "^10.3.8",
    "@types/express": "^4.17.21",
    "@types/jest": "29.5.12",
    "@types/node": "18.11.18",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "grpc-tools": "^1.12.4",
    "jest": "29.3.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
